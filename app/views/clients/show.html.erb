<h1><%= @client.name %></h1>

<table class="table"> 
  <tr>
    <th>Video URL</th>
  </tr>
  <% @client.spots.each do |spot| %>
  <tr>
    <td><%= spot.video_url %></td>
  </tr>
  <% end %>
</table>

<%= link_to "All Clients", clients_url %>
<%= link_to "Edit", edit_client_url(@client) %>
<%= link_to "New Spot", new_client_spot_url(@client) %>
